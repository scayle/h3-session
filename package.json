{
  "name": "@scayle/h3-session",
  "version": "0.5.1",
  "description": "Persistent sessions for h3",
  "author": "SCAYLE Commerce Engine",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "files": [
    "CHANGELOG.md",
    "dist/**"
  ],
  "scripts": {
    "build": "unbuild",
    "verify-packaging": "attw --pack . --profile esm-only",
    "format": "dprint check",
    "format:fix": "dprint fmt",
    "lint": "eslint . --format gitlab",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:ci": "vitest --run --coverage --reporter=default --reporter=junit --outputFile=./coverage/junit.xml",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "peerDependencies": {
    "h3": "^1.10.0"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.10.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.3",
    "@scayle/eslint-config-storefront": "4.4.1",
    "cookie-es": "1.2.2",
    "dprint": "0.48.0",
    "eslint": "9.18.0",
    "eslint-formatter-gitlab": "5.1.0",
    "h3": "1.13.0",
    "typescript": "5.7.3",
    "unbuild": "3.3.1",
    "vitest": "2.1.8"
  },
  "volta": {
    "node": "22.13.0"
  }
}
